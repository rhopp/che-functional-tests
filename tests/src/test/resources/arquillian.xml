<?xml version="1.0" encoding="UTF-8"?>
<arquillian xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://jboss.org/schema/arquillian"
    xsi:schemaLocation="http://jboss.org/schema/arquillian http://jboss.org/schema/arquillian/arquillian_1_0.xsd">

  <extension qualifier="che">
    <property name="cheStarterURL"></property>
    <property name="openShiftMasterURL"></property>
    <property name="openShiftNamespace"></property>
  </extension>
  
  <extension qualifier="webdriver">
    <property name="browser">chrome</property>
  </extension>

  <extension qualifier="graphene">
    <property name="waitGuiInterval">5</property>
    <property name="waitAjaxInterval">15</property>
    <property name="waitModelInterval">30</property>
  </extension>

</arquillian>
